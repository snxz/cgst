{
    "name": "Catlin Schedule Themer",
    "version": "0.0.1",
    "manifest_version": 2,
    "content_scripts": [
        {
            "matches": ["http://inside.catlin.edu/schedule", "https://inside.catlin.edu/schedule"],
            "js": ["/content_scripts.js"]
        }
    ],

    "browser_action": {
        "browser_style": true,
        "default_icon": {
            "48": "/favicon.png"
        },
        "default_title": "Schedule Themer",
        "default_popup": "/index.html"
    },

    "permissions": [
        "storage",
        "activeTab"
    ]
}